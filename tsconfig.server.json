    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": "./dist/server",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx" 
  },
  "include": ["src/server"]
}
